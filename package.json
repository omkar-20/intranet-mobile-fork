{
  "name": "Intranet",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=devdebug --appIdSuffix=dev",
    "android-dev-release": "react-native run-android --variant=devrelease --appIdSuffix=dev",
    "android-stage-debug": "react-native run-android --variant=stagedebug --appIdSuffix=stage",
    "android-stage-release": "react-native run-android --variant=stagerelease --appIdSuffix=stage",
    "android-prod-debug": "react-native run-android --variant=proddebug",
    "android-prod-release": "react-native run-android --variant=prodrelease",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "updateversion": "react-native-version --never-amend",
    "test": "jest",
    "clean": "./android/gradlew -p android clean",
    "bundle:prod": "./android/gradlew -p android bundleProdRelease",
    "assemble:prod": "./android/gradlew -p android assembleProdRelease",
    "build:prod": "npm run bundle:prod && npm run assemble:prod"
  },
  "dependencies": {
